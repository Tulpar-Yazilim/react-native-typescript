{
  "name": "boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android warning-mode=all",
    "ios": "react-native run-ios",
    "pod": "npx pod-install ios",
    "clean:android": "cd android && ./gradlew clean --warning-mode all && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && cd ..",
    "apk-mac": "cd android && ./gradlew assembleRelease  && cd ..",
    "apk-windows": "cd android && gradlew assembleRelease  && cd ..",
    "start": "react-native start",
    "start-reset-cache": "npm run start -- --reset-cache",
    "test": "jest",
    "lint": "tsc --noEmit && eslint --fix --ext .ts,.tsx ./app/**/*.{ts,tsx}",
    "pre-commit": "arn format && yarn lint && yarn tsc",
    "format": "prettier --write **/*.{js,ts,tsx,json} && prettier --check **/*.{js,ts,tsx,json}",
    "reset-cache": "npm start -- --reset-cache",
    "logcat": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "icon-list": "node setup/icon-list.js",
    "app:theme": "node setup/theme.js",
    "app:name": "npx react-native-rename boilerplate -b com.boilerplate",
    "xcode": "cd ios && xed . && cd ..",
    "dev-clean": "rm -rf ~/Library/Developer/Xcode/DerivedData",
    "app:asset": "npx react-native-asset",
    "swagger-generator": "npx @rtk-query/codegen-openapi openapi-config.json"
  },
  "dependencies": {
    "@baronha/react-native-multiple-image-picker": "^1.0.0",
    "@gorhom/bottom-sheet": "^4.4.7",
    "@hookform/resolvers": "^3.1.1",
    "@notifee/react-native": "^7.8.0",
    "@react-native-community/datetimepicker": "^7.3.0",
    "@react-native-community/netinfo": "^9.4.1",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@react-navigation/stack": "^6.3.17",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/lodash": "^4.14.195",
    "i18next": "^23.2.6",
    "lodash": "^4.17.21",
    "lottie-react-native": "^5.1.6",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-native": "0.72.1",
    "react-hook-form": "^7.45.1",
    "react-i18next": "^13.0.1",
    "react-native-calendars": "^1.1299.0",
    "react-native-collapsible": "^1.6.1",
    "react-native-crypto-js": "^1.0.0",
    "react-native-device-info": "^10.7.0",
    "react-native-dotenv": "^3.4.9",
    "react-native-drop-shadow": "^0.0.7",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-icomoon": "^0.1.1",
    "react-native-image-picker": "^5.6.0",
    "react-native-image-progress": "^1.2.0",
    "react-native-image-resizer": "^1.4.5",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.7.3",
    "react-native-modal": "^13.0.1",
    "react-native-modal-datetime-picker": "^17.0.0",
    "react-native-onesignal": "^4.5.1",
    "react-native-permissions": "^3.8.3",
    "react-native-portalize": "^1.0.7",
    "react-native-reanimated": "3.3.0",
    "react-native-render-html": "^6.3.4",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^4.6.3",
    "react-native-screens": "^3.22.0",
    "react-native-svg": "^13.9.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-toast-message": "^2.1.6",
    "react-redux": "^8.1.1",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "rn-placeholder": "^3.0.3",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.6",
    "@babel/preset-env": "^7.22.6",
    "@babel/runtime": "^7.22.6",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.7",
    "@tsconfig/react-native": "^3.0.2",
    "@types/metro-config": "^0.76.3",
    "@types/jest": "^29.5.2",
    "@types/md5": "^2.3.2",
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.14",
    "@types/react-native-crypto-js": "^1.0.0",
    "@types/react-redux": "^7.1.25",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "babel-jest": "^29.5.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-traverse": "^6.26.0",
    "babylon": "^6.18.0",
    "eslint": "^8.44.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.3",
    "metro-react-native-babel-preset": "0.76.7",
    "prettier": "^2.8.8",
    "react-native-obfuscator": "^1.0.0",
    "react-native-rename": "^3.2.13",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "app/**/*.{ts,tsx}": [
      "yarn lint"
    ],
    "./app/**": [
      "yarn format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=16"
  }
}
