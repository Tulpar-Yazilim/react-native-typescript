{
  "name": "boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android warning-mode=all",
    "ios": "react-native run-ios",
    "pod": "npx pod-install ios",
    "clean:android": "cd android && ./gradlew clean --warning-mode all && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && cd ..",
    "apk-mac": "cd android && ./gradlew assembleRelease  && cd ..",
    "apk-windows": "cd android && gradlew assembleRelease  && cd ..",
    "start": "react-native start",
    "start-reset-cache": "npm run start -- --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "reset-cache": "npm start -- --reset-cache",
    "logcat": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "icon-list": "node setup/icon-list.js",
    "app:theme": "node setup/theme.js",
    "app:name": "npx react-native-rename boilerplate -b com.boilerplate",
    "xcode": "cd ios && xed . && cd ..",
    "dev-clean": "rm -rf ~/Library/Developer/Xcode/DerivedData",
    "app:asset": "npx react-native-asset",
    "swagger-generator": "npx @rtk-query/codegen-openapi openapi-config.json"
  },
  "dependencies": {
    "@baronha/react-native-multiple-image-picker": "^0.4.13",
    "@gorhom/bottom-sheet": "^4.4.5",
    "@hookform/resolvers": "^2.9.10",
    "@notifee/react-native": "^7.3.0",
    "@react-native-community/datetimepicker": "^6.7.1",
    "@react-native-community/netinfo": "^9.3.7",
    "@react-navigation/bottom-tabs": "^6.5.2",
    "@react-navigation/drawer": "^6.5.6",
    "@react-navigation/native": "^6.1.1",
    "@react-navigation/native-stack": "^6.9.7",
    "@react-navigation/stack": "^6.3.10",
    "@reduxjs/toolkit": "^1.9.1",
    "axios": "^1.2.1",
    "i18next": "^22.4.6",
    "lodash": "^4.17.21",
    "lottie-react-native": "^5.1.4",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "react": "18.1.0",
    "react-hook-form": "^7.41.5",
    "react-i18next": "^12.1.1",
    "react-native": "0.70.6",
    "react-native-calendars": "^1.1292.0",
    "react-native-collapsible": "^1.6.0",
    "react-native-crypto-js": "^1.0.0",
    "react-native-dotenv": "^3.4.6",
    "react-native-drop-shadow": "^0.0.6",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-icomoon": "^0.1.1",
    "react-native-image-picker": "^4.10.3",
    "react-native-image-progress": "^1.2.0",
    "react-native-image-resizer": "^1.4.5",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-modal": "^13.0.1",
    "react-native-modal-datetime-picker": "^14.0.1",
    "react-native-onesignal": "^4.5.0",
    "react-native-permissions": "^3.6.1",
    "react-native-portalize": "^1.0.7",
    "react-native-reanimated": "3.0.0-rc.9",
    "react-native-render-html": "^6.3.4",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-svg": "^13.6.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-toast-message": "^2.1.5",
    "react-redux": "^8.0.5",
    "redux": "^4.2.0",
