{
  "name": "reactnativetypescript",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "pod": "npx pod-install ios",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "apk-mac": "cd android && ./gradlew assembleRelease  && cd ..",
    "apk-windows": "cd android && gradlew assembleRelease  && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && cd ..",
    "start": "react-native start",
    "start-reset-cache": "npm run start -- --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "logcat": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "icon": "npx react-native set-icon --path ./src/assets/images/app-icon.png",
    "codepush:deploymentKey-android": "appcenter codepush deployment list --app ******* -k",
    "codepush:deploymentKey-ios": "appcenter codepush deployment list --app ******* -k",
    "codepush:ios": "appcenter codepush release -a ******* -d Production -c ./codePush/ios -t 1.0",
    "codepush:android": "appcenter codepush release -a ******* -d Production -c ./codePush/android -t 1.0",
    "codepush-release-android": "react-native bundle --platform android --entry-file index.js --bundle-output ./codePush/android/index.android.bundle --assets-dest ./codePush/android --dev false",
    "codepush-release-ios": "react-native bundle --platform ios --entry-file index.js --bundle-output ./codePush/ios/index.ios.bundle --assets-dest ./codePush/ios --dev false"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "axios": "^0.21.4",
    "i18n-js": "^3.8.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react": "17.0.2",
    "react-native": "0.65.1",
    "react-native-code-push": "^7.0.4",
    "react-native-crypto-js": "^1.0.0",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-localize": "^2.1.4",
    "react-native-modal": "^13.0.0",
    "react-native-permissions": "^3.1.0",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.8.0",
    "react-native-vector-icons": "^8.1.0",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0",
    "rn-placeholder": "^3.0.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@bam.tech/react-native-make": "^3.0.3",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/i18n-js": "^3.8.2",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.65.0",
    "@types/react-native-vector-icons": "^6.4.9",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-traverse": "^6.26.0",
    "babylon": "^6.18.0",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-native-codegen": "^0.0.7",
    "react-native-obfuscator": "^1.0.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
