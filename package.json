{
  "name": "boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android warning-mode=all",
    "ios": "react-native run-ios",
    "pod": "npx pod-install ios",
    "clean:android": "cd android && ./gradlew clean --warning-mode all && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && cd ..",
    "apk-mac": "cd android && ./gradlew assembleRelease  && cd ..",
    "apk-windows": "cd android && gradlew assembleRelease  && cd ..",
    "start": "react-native start",
    "start-reset-cache": "npm run start -- --reset-cache",
    "test": "jest",
    "lint": "tsc --noEmit && eslint --fix --ext .ts,.tsx ./app/**/*.{ts,tsx}",
    "pre-commit": "yarn lint && yarn format",
    "format": "prettier --write **/*.{js,ts,tsx,json} && prettier --check **/*.{js,ts,tsx,json}",
    "reset-cache": "npm start -- --reset-cache",
    "logcat": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "icon-list": "node setup/icon-list.js",
    "app:theme": "node setup/theme.js",
    "app:name": "npx react-native-rename boilerplate -b com.boilerplate",
    "xcode": "cd ios && xed . && cd ..",
    "dev-clean": "rm -rf ~/Library/Developer/Xcode/DerivedData",
    "app:asset": "npx react-native-asset",
    "swagger-generator": "npx @rtk-query/codegen-openapi openapi-config.json"
  },
  "dependencies": {
    "@baronha/react-native-multiple-image-picker": "^1.0.0",
    "@gorhom/bottom-sheet": "^4.4.7",
    "@hookform/resolvers": "^3.1.0",
    "@notifee/react-native": "^7.7.1",
    "@react-native-community/datetimepicker": "^7.1.0",
    "@react-native-community/netinfo": "^9.3.10",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/drawer": "^6.6.2",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/stack": "^6.3.16",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/lodash": "^4.14.195",
    "i18next": "^22.5.1",
    "lodash": "^4.17.21",
    "lottie-react-native": "^5.1.6",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-hook-form": "^7.44.3",
    "react-i18next": "^12.3.1",
    "react-native": "0.71.10",
    "react-native-calendars": "^1.1298.0",
    "react-native-collapsible": "^1.6.1",
    "react-native-crypto-js": "^1.0.0",
    "react-native-device-info": "^10.6.0",
    "react-native-dotenv": "^3.4.8",
    "react-native-drop-shadow": "^0.0.7",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.11.0",
    "react-native-icomoon": "^0.1.1",
    "react-native-image-picker": "^5.4.0",
    "react-native-image-progress": "^1.2.0",
    "react-native-image-resizer": "^1.4.5",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-modal": "^13.0.1",
    "react-native-modal-datetime-picker": "^15.0.1",
    "react-native-onesignal": "^4.5.1",
    "react-native-permissions": "^3.8.0",
    "react-native-portalize": "^1.0.7",
    "react-native-reanimated": "3.2.0",
    "react-native-render-html": "^6.3.4",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-screens": "^3.20.0",
    "react-native-svg": "^13.9.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-toast-message": "^2.1.6",
    "react-redux": "^8.0.7",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "rn-placeholder": "^3.0.3",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/md5": "^2.3.2",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.24",
    "@types/react-native-crypto-js": "^1.0.0",
    "@types/react-redux": "^7.1.24",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-traverse": "^6.26.0",
    "babylon": "^6.18.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^26.6.3",
    "lint-staged": "^13.1.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.4.1",
    "react-native-obfuscator": "^1.0.0",
    "react-native-rename": "^2.9.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "app/**/*.{ts,tsx}": [
      "yarn lint"
    ],
    "./app/**": [
      "yarn format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
